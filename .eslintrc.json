{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "prettier",
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": "warn"
  },
  "root": true
}
